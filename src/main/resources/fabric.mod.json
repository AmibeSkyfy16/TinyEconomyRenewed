{
  "schemaVersion": 1,
  "id": "tinyeconomyrenewed",
  "version": "${version}",
  "name": "Tiny Economy Renewed",
  "description": "A tiny economy system, players can sell/buy items between them by creating shop. Players can also earn money from actions like destroying blocks, killing entities and realising advancements",
  "authors": [
    "Skyfy16"
  ],
  "contact": {},
  "license": "MIT",
  "icon": "assets/tinyeconomyrenewed/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "ch.skyfy.tinyeconomyrenewed.both.TinyEconomyRenewedMod"
      }
    ],
    "server": [
      {
        "adapter": "kotlin",
        "value": "ch.skyfy.tinyeconomyrenewed.server.TinyEconomyRenewedModServer"
      }
    ],
    "client": [
      {
        "adapter": "kotlin",
        "value": "ch.skyfy.tinyeconomyrenewed.client.TinyEconomyRenewedModClient"
      }
    ]
  },
  "mixins": [
    "tinyeconomyrenewed.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.14.17",
    "fabric": "*",
    "minecraft": "1.19.3",
    "fabric-language-kotlin": ">=1.9.1+kotlin.1.8.10",
    "silk-game": ">=1.9.6",
    "mariadb_server_fabricmc": ">=0.0.1+1.19.3"
  }
}
